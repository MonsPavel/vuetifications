{
  "name": "vuetifications",
  "private": false,
  "version": "1.4.7",
  "type": "module",
  "main": "./dist/vuetifications.umd.js",
  "module": "./dist/vuetifications.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vuetifications.es.js",
      "require": "./dist/vuetifications.umd.js"
    },
    "./styles/vuetifications.css": "./dist/vuetifications.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Mons Pavel <monspavel@gmail.com>",
  "description": "Vuejs notifications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MonsPavel/vuetifications.git"
  },
  "keywords": [
    "vue",
    "toastr",
    "toast",
    "non-blocking",
    "notifications",
    "notification",
    "vuejs",
    "vue.js",
    "messages",
    "errors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MonsPavel/vuetifications/issues"
  },
  "homepage": "https://github.com/MonsPavel/vuetifications#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint ./ && vue-tsc --noEmit",
    "lint:fix": "npm run lint --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "rm -rf docs && storybook build -o docs"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.5",
    "@semantic-release/npm": "^12.0.2",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/vue3-vite": "^9.1.3",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "^10.4.0",
    "playwright": "^1.55.0",
    "semantic-release": "^24.2.7",
    "storybook": "^9.1.3",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
}
